Idle = true
function onWalk(spd)
  if spd >= 1.0E-4 then
    Walk = true
    Idle = false
    Jump = false
    Fall = false
    Swim = false
    Stun = false
  else
    Walk = false
    Idle = true
    Jump = false
    Fall = false
    Swim = false
    Stun = false
  end
end
function onJump()
  Walk = false
  Idle = false
  Jump = true
  Fall = false
  Swim = false
  Stun = false
  wait(0.25)
  Fall = true
  Jump = false
end
function onFall()
  wait(0.25)
  Walk = false
  Idle = false
  Jump = false
  Fall = true
  Swim = false
  Stun = false
end
function onSwim()
  Walk = false
  Idle = false
  Jump = false
  Fall = false
  Swim = true
  Stun = false
end
function onStunned()
  Walk = false
  Idle = false
  Jump = false
  Fall = false
  Swim = false
  Stun = true
end
function doattack1()
  if AttackPress == false then
    AttackPress = true
    print("Attacking")
    Humanoid.WalkSpeed = 20
    Humanoid.JumpPower = 120
    Walk = false
    Idle = false
    Jump = false
    Fall = false
    Swim = false
    Stun = false
    wait(0.1)
    attack1anim = true
    attack2anim = false
    wait(0.25)
    attack1anim = false
    attack2anim = true
    SmashinClub.Transparency = 1
    do
      local newclub = SmashinClub:Clone()
      newclub.Position = RightArm.Position
      newclub.Parent = workspace
      newclub.Transparency = 0
      newclub.Anchored = false
      local targetpos = Instance.new("Part")
      targetpos.Anchored = true
      targetpos.Size = Vector3.new(0, 0, 0)
      targetpos.Parent = workspace
      targetpos.CFrame = Mouse.Hit
      targetpos.Name = "Target"
      targetpos.CanCollide = false
      targetpos.Transparency = 1
      newclub.Position = targetpos.Position
      local explosion = Instance.new("Explosion", newclub)
      explosion.BlastPressure = 0
      explosion.BlastRadius = 2
      explosion.DestroyJointRadiusPercent = 0
      explosion.Position = newclub.Position
      function soundeffect()
        local soundeff = Instance.new("Sound", newclub)
        soundeff.SoundId = "rbxassetid://320242352"
        soundeff.Volume = 1
        soundeff:Play()
        soundeff.Ended:connect(function()
          soundeff:Destroy()
        end)
      end
      soundeffect()
      newclub.Touched:connect(function(part)
        local plr = part.Parent
        local hum = plr.Humanoid
        if hum then
          plr:BreakJoints()
          hum.Health = 0
        end
      end)
      wait(0.5)
      Idle = true
      targetpos:Destroy()
      repeat
        wait()
        SmashinClub.Transparency = SmashinClub.Transparency - 0.025
      until SmashinClub.Transparency <= 0
      newclub:Destroy()
      attack2anim = false
      attack1anim = false
      Humanoid.WalkSpeed = 16
      Humanoid.JumpPower = 50
      AttackPress = false
      attack2anim = false
      attack1anim = false
      Idle = true
    end
  else
    print("Aw you cant attack yet ;(")
    Humanoid.Health = Humanoid.Health - 4
  end
end
SmashinClub.Anchored = false
function Shielded()
  if AttackPress == false then
    AttackPress = true
    AttackClick = true
    Humanoid.WalkSpeed = 0
    Humanoid.JumpPower = 0
    Idle = false
    Walk = false
    Jump = false
    Fall = false
    Swim = false
    Stun = false
    wait(0.1)
    ShieldAnim = true
    local Shieldstuf = Instance.new("ForceField", Character)
    local YEYYE = Instance.new("ScreenGui")
    local YOOGE = Instance.new("Frame")
    local Text1 = Instance.new("TextLabel")
    local Text2 = Instance.new("TextLabel")
    local Num = Instance.new("TextLabel")
    YEYYE.Name = "YEYYE"
    YEYYE.Parent = Player_UI
    YOOGE.Name = "YOOGE"
    YOOGE.Parent = YEYYE
    YOOGE.BackgroundColor3 = Color3.new(1, 1, 1)
    YOOGE.Draggable = true
    YOOGE.Position = UDim2.new(0.623232305, 0, 0.468899518, 0)
    YOOGE.Size = UDim2.new(0.304040402, 0, 0.270334929, 0)
    YOOGE.Style = Enum.FrameStyle.DropShadow
    Text1.Name = "Text1"
    Text1.Parent = YOOGE
    Text1.BackgroundColor3 = Color3.new(0, 0, 0)
    Text1.BackgroundTransparency = 1
    Text1.Position = UDim2.new(0.0166112948, 0, 0.345132738, 0)
    Text1.Size = UDim2.new(0.455149502, 0, 0.309734523, 0)
    Text1.ZIndex = 2
    Text1.Font = Enum.Font.SourceSans
    Text1.Text = "You have "
    Text1.TextColor3 = Color3.new(1, 1, 1)
    Text1.TextScaled = true
    Text1.TextStrokeTransparency = 0
    Text1.TextWrapped = true
    Text2.Name = "Text2"
    Text2.Parent = YOOGE
    Text2.BackgroundColor3 = Color3.new(0, 0, 0)
    Text2.BackgroundTransparency = 1
    Text2.Position = UDim2.new(0.581395328, 0, 0.14159292, 0)
    Text2.Size = UDim2.new(0.398671091, 0, 0.743362844, 0)
    Text2.ZIndex = 2
    Text2.Font = Enum.Font.SourceSans
    Text2.Text = "Seconds Until your shield goes"
    Text2.TextColor3 = Color3.new(1, 1, 1)
    Text2.TextScaled = true
    Text2.TextStrokeTransparency = 0
    Text2.TextWrapped = true
    Num.Name = "Num"
    Num.Parent = YOOGE
    Num.BackgroundColor3 = Color3.new(0, 0, 0)
    Num.BackgroundTransparency = 1
    Num.Position = UDim2.new(0.405315608, 0, 0.300884962, 0)
    Num.Size = UDim2.new(0.239999995, 0, 0.370000005, 0)
    Num.ZIndex = 2
    Num.Font = Enum.Font.SourceSans
    Num.Text = "5"
    Num.TextColor3 = Color3.new(1, 1, 1)
    Num.TextScaled = true
    Num.TextStrokeTransparency = 0
    Num.TextWrapped = true
    for i = 5, 0, -1 do
      wait(1)
      Num.Text = tostring(Num.Text - 1)
      if 0 >= tonumber(Num.Text) then
        YEYYE:Destroy()
        Shieldstuf:Destroy()
        break
      end
    end
    Idle = true
    ShieldAnim = false
    Humanoid.WalkSpeed = 20
    Humanoid.JumpPower = 50
    AttackClick = false
    AttackPress = false
  else
    print("Please wait till you can use your shield ", Player.Name)
  end
end
function SoundCLANGClub()
  local soundeff = Instance.new("Sound", SmashinClub)
  soundeff.Pitch = 1
  soundeff.Volume = 2
  soundeff.SoundId = id .. "319998195"
  soundeff:Play()
  soundeff.Ended:connect(function()
    soundeff:Destroy()
  end)
end
function CLUBSMACKBOIIII()
  if AttackPress == false then
    AttackPress = true
    AttackClick = true
    print("Attacking!")
    Idle = false
    Walk = false
    Jump = false
    Fall = false
    Swim = false
    Stun = false
    Humanoid.WalkSpeed = 0
    Humanoid.JumpPower = 120
    wait(0.1)
    GobackAnim = true
    SmackGroundAnim = false
    wait(0.3)
    GobackAnim = false
    SmackGroundAnim = true
    SoundCLANGClub()
    Root.Anchored = true
    local function DESTROYALLNEARBY()
      function touchedmyClub(part)
        if AttackPress == true then
          local plr = part.Parent
          local hum = plr.Humanoid
          if hum then
            if plr.Name == Player.Name then
              print("Sorry Master")
            else
              local bodyvel = Instance.new("BodyVelocity")
              bodyvel.MaxForce = Vector3.new(0, 8000, 0)
              bodyvel.P = 1250
              bodyvel.Velocity = Vector3.new(1, 4, 1)
              hum.PlatformStand = true
              hum.Health = hum.Health - 0.7
              wait(0.2)
              bodyvel:Destroy()
            end
          end
        end
      end
      SmashinClub.Touched:connect(touchedmyClub)
      local explogen = Instance.new("Explosion", SmashinClub)
      explogen.BlastPressure = 700000
      explogen.DestroyJointRadiusPercent = 0
      explogen.ExplosionType = Enum.ExplosionType.CratersAndDebris
      explogen.BlastRadius = 2
      explogen.Position = SmashinClub.Position
      explogen.Hit:connect(function(plr)
        local checkplr = plr.Parent
        local checkplrhum = checkplr.Humanoid
        if checkplrhum then
          if checkplr.Name == Player.Name then
            print("SORRY!!")
          else
            local bodyvel = Instance.new("BodyVelocity")
            bodyvel.MaxForce = Vector3.new(0, 8000, 0)
            bodyvel.P = 1250
            bodyvel.Velocity = Vector3.new(2, 4, 2)
            checkplrhum.PlatformStand = true
            checkplrhum.Health = checkplrhum.Health - 0.9
            wait(0.2)
            bodyvel:Destroy()
          end
        end
      end)
    end
    wait(0.2)
    DESTROYALLNEARBY()
    wait(0.5)
    GobackAnim = false
    SmackGroundAnim = false
    Idle = true
    Root.Anchored = false
    AttackClick = false
    AttackPress = false
    Humanoid.WalkSpeed = 20
    Humanoid.JumpPower = 50
  else
    print("Wait your turn..")
  end
end
Mouse.KeyDown:connect(function(keky)
  if keky == "e" then
    doattack1()
  elseif keky == "q" then
    Shielded()
  end
end)
Mouse.Button1Down:connect(function()
  CLUBSMACKBOIIII()
end)
Suiscide.MouseButton1Down:connect(function()
  Character:BreakJoints()
  Suiscide.Text = "Rip " .. Player.Name
end)
Humanoid.Running:connect(onWalk)
Humanoid.Jumping:connect(onJump)
Humanoid.Swimming:connect(onSwim)
Humanoid.PlatformStanding:connect(onStunned)
Humanoid.Died:connect(function()
  local function soundeffdead()
    local soundeff = Instance.new("Sound", Head)
    soundeff.SoundId = "rbxassetid://306323388"
    soundeff.Volume = 1
    soundeff.Pitch = 1.4
    soundeff:Play()
    print("Ded")
  end
  soundeffdead()
  wait(2)
  Player.Respawn()
end)
